{
  "name": "cleanplate",
  "version": "0.1.8",
  "description": "CleanPlate - A Headless React UI Framework",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "storybook": "storybook dev -p 6001",
    "build-storybook": "storybook build",
    "build-package": "rollup -c && npm run build-types",
    "build-types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "generate-llms-txt": "storybook-llms-extractor --config llms.config.js",
    "generate-icons": "node scripts/generate-icons.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sivadass/cleanplate.git"
  },
  "keywords": [
    "react",
    "ui",
    "headless"
  ],
  "author": "Sivadass N",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sivadass/cleanplate/issues"
  },
  "homepage": "https://cleanplate.sivadass.in",
  "devDependencies": {
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.28.5",
    "@floating-ui/react": "^0.27.16",
    "@fluentui/storybook-llms-extractor": "^0.0.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "baseline-browser-mapping": "^2.9.16",
    "cheerio": "^1.2.0",
    "node-fetch": "^2.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.72.0",
    "storybook": "^7.5.3",
    "typescript": "^5.9.3"
